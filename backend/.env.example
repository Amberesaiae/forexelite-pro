# ForexElite Pro - Backend Environment Variables
# For full configuration, see the root .env.example file

# =============================================================================
# These variables are specific to the backend service
# =============================================================================

# Supabase (required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here
SUPABASE_ANON_KEY=your_anon_key_here

# Redis (required)
REDIS_URL=redis://localhost:6379

# GLM-5 API (ZhipuAI/GLM-4) - for EA code generation
# Get API key from: https://open.bigmodel.cn/
GLM5_API_KEY=your_glm5_api_key_here
GLM5_API_BASE_URL=https://open.bigmodel.cn/api/paas/v4

# CORS (comma-separated list of origins)
CORS_ORIGINS=http://localhost:3000

# Environment
ENVIRONMENT=development
DEBUG=true

# Base URL (for webhook construction)
BASE_URL=http://localhost:8000
